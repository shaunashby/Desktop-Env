;; XEmacs support

(defconst eperiodic-xemacs-p
  (or (featurep 'xemacs)
      (string-match "XEmacs\\|Lucid" (emacs-version)))
  "True if we are using eperiodic under XEmacs.")

;; Entry point

;;;###autoload
(defun eperiodic ()
  "Display the periodic table of the elements in its own buffer."
  (interactive)
  (set-buffer (get-buffer-create "*EPeriodic*"))
  (eperiodic-mode)
  (setq buffer-read-only t)
  (setq truncate-lines t)
  (let ((display-width (max eperiodic-element-display-width 3))
        (indentation (max eperiodic-display-indentation 0))
        (inhibit-read-only t)
        (max-width 0)
        (order (cdr (assoc eperiodic-display-type eperiodic-display-lists)))
        (separation (max eperiodic-element-separation 0)))
    (erase-buffer)
    ;; Loop over display order
    (while order
      (insert (make-string indentation ?\ ))
      (let ((orbital-list (car order))
            (width 0)
            face max-z min-z orbital z)
        (while orbital-list
          (setq orbital (car orbital-list))
          (setq min-z (cadr (assoc orbital eperiodic-orbital-z-value-map)))
          (setq max-z (cddr (assoc orbital eperiodic-orbital-z-value-map)))
          ;; Get the face for this block
          (setq face (cdr (assoc (substring (symbol-name orbital) 1 2)
                                 eperiodic-orbital-faces)))
          ;; Print the range of Z for this orbital
          (cond
           (min-z
            (loop for z from min-z to max-z by 1
                  do
                  (let ((help (if eperiodic-use-popup-help
                                  (eperiodic-get-help-string z)
                                nil))
                        (start (point)))
                    (insert (format (format "%%-%ds" (+ separation
                                                        display-width))
                                    (eperiodic-get-element-property z 'symbol)))
                    (add-text-properties start (- (point) separation)
                                         `(face ,face
                                                eperiodic-at-number ,z
                                                mouse-face highlight
                                                help-echo ,help))
                    ;; Track width
                    (setq width (1+ width)))))
           ;; Must be a 0x type orbital, i.e. padding
           (t
            (let ((string (make-string (+ separation display-width) ?\ )))
              (add-text-properties 0 display-width
                                   '(face eperiodic-padding-face)
                                   string)
              (loop for z from 1 to
                    (cdr (assoc (substring (symbol-name orbital) 1 2)
                                eperiodic-orbital-degeneracies)) by 1
                    do
                    (insert string)))
            (setq width (1+ width))))
          (setq orbital-list (cdr orbital-list)))
        ;; Keep track of maximum width
        (if (> width max-width)
            (setq max-width width))
        (insert "\n"))
      (setq order (cdr order)))
    ;; Insert header
    (let* ((header "Periodic Table of the Elements")
           (padding (/ (- (+ (* max-width (+ display-width
                                             separation))
                             indentation) (length header)) 2)))
      (goto-char (point-min))
      (insert (make-string padding ?\ ))
      (add-text-properties 0 (length header) `(face eperiodic-header-face)
                           header)
      (insert header "\n\n"))
    ;; Align He above Ne if both are shown
    (let ((he-posn
           (text-property-any (point-min) (point-max) 'eperiodic-at-number 2))
          (ne-posn
           (text-property-any (point-min) (point-max) 'eperiodic-at-number 10)))
      (when (and ne-posn he-posn)
        ;; Convert positions to columns
        (goto-char he-posn)
        (let ((string (make-string (+ separation display-width) ?\ )))
          (add-text-properties 0 display-width
                               '(face eperiodic-padding-face)
                               string)
          (loop for i from 1 to (- max-width 2)
                do
		;; Need the following in XEmacs to avoid some
		;; weirdness associated with text properties.
		(when eperiodic-xemacs-p
		  (goto-char (1- he-posn)))
                (insert string))))))
  (set-buffer-modified-p nil)
  (select-window (display-buffer (get-buffer "*EPeriodic*")))
  (delete-other-windows)
  ;; Move to point after first element
  (goto-char (next-single-property-change (point-min) 'eperiodic-at-number)))

;; Functions

(defun eperiodic-show-element-info ()
  "Show data for the element at point."
  (interactive)
  (let ((z (eperiodic-element-at)))
    (eperiodic-display-element-info z)))

(defun eperiodic-show-isotope-info ()
  "Show isotope data for the element at point."
  (interactive)
  (let ((z (eperiodic-element-at)))
    (eperiodic-display-isotope-info z)))

(defun eperiodic-mouse-show-element-info (event)
  "Show data for the element at mouse click.
Argument EVENT is a mouse event."
  (interactive "e")
  (set-buffer "*EPeriodic*")         ; cursor may be in another buffer
  (let ((z (eperiodic-element-at
            (if eperiodic-xemacs-p
                (event-point event)
              (posn-point (event-start event))))))
    (eperiodic-display-element-info z)))

(defun eperiodic-mouse-show-isotope-info (event)
  "Show isotope data for the element at mouse click.
Argument EVENT is a mouse event."
  (interactive "e")
  (set-buffer "*EPeriodic*")         ; cursor may be in another buffer
  (let ((z (eperiodic-element-at
            (if eperiodic-xemacs-p
                (event-point event)
              (posn-point (event-start event))))))
    (eperiodic-display-isotope-info z)))

(defun eperiodic-display-element-info (z)
  "Display data for the element with atomic number Z."
  (let ((inhibit-read-only t))
    (if (not z)
        (error "Not on element"))
    (set-buffer (get-buffer-create "*EPeriodic Element Properties*"))
    (setq buffer-read-only t)
    (erase-buffer)
    (eperiodic-insert-element-info z)
    (set-buffer-modified-p nil)
    (goto-char (point-min))
    (display-buffer (get-buffer "*EPeriodic Element Properties*"))
    (let ((window (display-buffer "*EPeriodic*")))
      (set-buffer "*EPeriodic*")        ; For (point-{min,max})
      (save-selected-window
        (select-window window)
        (shrink-window (- (window-height) 2
                          (count-lines (point-min) (point-max))))))))

(defun eperiodic-display-isotope-info (z)
  "Display isotope data for the element with atomic number Z."
  (let ((inhibit-read-only t))
    (if (not z)
        (error "Not on element"))
    (set-buffer (get-buffer-create "*EPeriodic Element Isotopes*"))
    (setq buffer-read-only t)
    (erase-buffer)
    (eperiodic-insert-isotope-info z)
    (set-buffer-modified-p nil)
    (goto-char (point-min))
    (display-buffer (get-buffer "*EPeriodic Element Isotopes*"))
    (let ((window (display-buffer "*EPeriodic*")))
      (set-buffer "*EPeriodic*")        ; For (point-{min,max})
      (save-selected-window
        (select-window window)
        (shrink-window (- (window-height) 2
                          (count-lines (point-min) (point-max))))))))

(defun eperiodic-insert-element-info (z)
  "Insert data for the element with atomic number Z.
The data (taken from `eperiodic-element-properties') are inserted into
the current buffer."
  ;; Insert atomic number
  (insert (format "%-30s %-25s\n"
                  "Atomic Number"
                  z))
  ;; Insert other properties
  (let ((properties eperiodic-printed-properties)
        property)
    (while properties
      (setq property (car properties))
      (insert
       (format "%-30s %-25s %-s\n"
               (eperiodic-format-symbol property)
               (eperiodic-get-element-property z (car properties))
               (eperiodic-get-property-unit (car properties))))
      (setq properties (cdr properties)))))

(defun eperiodic-insert-isotope-info (z)
  "Insert isotope data for the element with atomic number Z.
The data (taken from `eperiodic-isotope-data') are inserted into the
current buffer."
  ;; Header
  (insert (format "%s Isotopes (Z = %d)\n\n"
                  (eperiodic-get-element-property z 'name) z))
  (mapcar (lambda (property)
            (insert
             (format "%-25s"
                     (eperiodic-format-symbol property))))
          eperiodic-isotope-properties)
  (insert "\n\n")
  ;; Data
  (let ((data (cdr (assoc z eperiodic-isotope-data))))
    ;; Loop over isotopes
    (while data
      (let ((isotope-data (car data)))
        (while isotope-data
          (insert
           (format "%-25s"
                   (car isotope-data)))
          (setq isotope-data (cdr isotope-data)))
        (insert "\n"))
      (setq data (cdr data)))))

(defun eperiodic-bury-buffer ()
  "Bury the *EPeriodic* buffer."
  (interactive)
  (if (eq (window-buffer) (get-buffer "*EPeriodic*"))
      (if (fboundp 'quit-window)
          (quit-window)
        (bury-buffer))))

(defvar eperiodic-completion-table
  (let ((table
         (mapcar (lambda (w)
                   (cons (cdr (assoc 'name (cdr w)))
                         (car w)))
                 eperiodic-element-properties)))
    table)
  "Completion table mapping element names to atomic numbers.")

(defun eperiodic-find-element ()
  "Find a named element in the *EPeriodic* buffer."
  (interactive)
  (if (not (equal (buffer-name) "*EPeriodic*"))
      (error "Not in the *EPeriodic* buffer"))
  (let ((completion-ignore-case t)
        element)
    (setq element (completing-read
                   "Choose element: " eperiodic-completion-table nil t))
    (when (> (length element) 0)
      (goto-char
       (text-property-any (point-min)
                          (point-max)
                          'eperiodic-at-number
                          (cdr (assoc element
                                      eperiodic-completion-table)))))))

(defun eperiodic-get-element-property (z prop)
  "For element with atomic number Z, get property PROP."
  (cond ((equal prop 'electronic-configuration)
         (let ((exception
                (cdr (assoc z eperiodic-aufbau-exceptions))))
           (if exception
               (concat exception " (non-Aufbau)")
             (cdr (assoc z eperiodic-elec-configs)))))
        (t
         (cdr (assoc prop (cdr (assoc z eperiodic-element-properties)))))))

(defun eperiodic-get-property-unit (prop)
  "Get the unit (if any) for property PROP."
  (or (cdr (assoc prop eperiodic-stored-properties))
      ""))

(defun eperiodic-get-help-string (z)
  "Build help string for element with atomic number Z."
  (let ((mass (eperiodic-get-element-property z 'atomic-mass))
        (name (eperiodic-get-element-property z 'name)))
    (format "%s: Z = %d." name z mass)))

(defvar eperiodic-use-property-change
  (fboundp 'next-single-char-property-change)
  "If non-nil, use single character property change functions.")

;; Adapted from widget-move

(defun eperiodic-move-forward (&optional arg)
  "Move point to the ARG next element.
ARG may be negative to move backward."
  (interactive "p")
  (let ((pos (point))
	(number arg)
	(old (eperiodic-element-at))
	new)
    ;; Forward.
    (while (> arg 0)
      (cond ((eobp)
	     (goto-char (point-min)))
	    (eperiodic-use-property-change
	     (goto-char (next-single-char-property-change (point) 'eperiodic-at-number)))
            (t
             (forward-char 1)))
      (let ((new (eperiodic-element-at)))
	(when new
	  (unless (eq new old)
	    (setq arg (1- arg))
	    (setq old new)))))
    ;; Backward.
    (while (< arg 0)
      (cond ((bobp)
	     (goto-char (point-max)))
	    (eperiodic-use-property-change
	     (goto-char (previous-single-char-property-change (point) 'eperiodic-at-number)))
            (t
             (backward-char 1)))
      (let ((new (eperiodic-element-at)))
	(when new
	  (unless (eq new old)
	    (setq arg (1+ arg))))))
    ;; Go to beginning of field.
    (let ((new (eperiodic-element-at)))
      (while (eq (eperiodic-element-at) new)
	(backward-char)))
    (forward-char))
  ;; Echo some info.
  (eperiodic-element-at-message))

(defun eperiodic-move-backward (&optional arg)
  "Move point to the ARG previous element.
ARG may be negative to move forward."
  (interactive "p")
  (eperiodic-move-forward (- arg)))

(defun eperiodic-element-at (&optional posn)
  "Get atomic number text property at point, or POSN if specified."
  (get-text-property (or posn (point)) 'eperiodic-at-number))

(defun eperiodic-element-at-message ()
  "Emit message describing element at point."
  (let ((element (eperiodic-element-at)))
    (if element
        (message (eperiodic-get-help-string element)))))

(defun eperiodic-format-symbol (symbol)
  "Format `symbol-name' of SYMBOL.
Remove hyphens and capitalize using relevant functions if available."
  (let (name)
    (setq name (symbol-name symbol))
    (setq name
          (cond ((fboundp 'replace-regexp-in-string)
                 (replace-regexp-in-string "-" " " name))
                ((fboundp 'replace-in-string)
                 (replace-in-string name "-" " "))
                (t
                 name)))
    (capitalize name)))

;; Mode settings

(defvar eperiodic-mode-map
  (let ((map (make-sparse-keymap)))
    (define-key map (kbd "1")   'delete-other-windows)
    (define-key map (kbd "f")   'eperiodic-find-element)
    (define-key map (kbd "i")   'eperiodic-show-isotope-info)
    (define-key map (kbd "q")   'eperiodic-bury-buffer)
    (define-key map (kbd "r")   'eperiodic)
    (define-key map (kbd "?")   'describe-mode)
    (define-key map (kbd "RET") 'eperiodic-show-element-info)
    (define-key map (kbd "TAB") 'eperiodic-move-forward)
    (define-key map (kbd "M-TAB") 'eperiodic-move-backward)
    (if eperiodic-xemacs-p
        (progn
          (define-key map '(button1) 'eperiodic-mouse-show-isotope-info)
          (define-key map '(button2) 'eperiodic-mouse-show-element-info))
      (define-key map (kbd "<mouse-1>") 'eperiodic-mouse-show-isotope-info)
      (define-key map (kbd "<mouse-2>") 'eperiodic-mouse-show-element-info))
    map)
  "Keymap for analog mode.")

(defun eperiodic-mode ()
  "Major mode for controlling the *EPeriodic* buffer.

This buffer contains a representation of the periodic table of the
elements. Elemental and isotopic data can be displayed using the
keyboard or the mouse.

\\{eperiodic-mode-map}"
  (kill-all-local-variables)
  (use-local-map eperiodic-mode-map)
  (setq major-mode 'eperiodic-mode)
  (setq mode-name "EPeriodic")
  (run-hooks 'eperiodic-mode-hook))

(provide 'eperiodic)

